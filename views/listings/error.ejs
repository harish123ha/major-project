<%- layout("layouts/boilerplate")%>

<div class="row mt-3">
  <div
    class="alert alert-danger d-flex align-items-center col-8 offset-2"
    role="alert"
  >
    <svg
      class="bi flex-shrink-0 me-2"
      width="24"
      height="24"
      role="img"
      aria-label="Danger:"
    >
      <use xlink:href="#exclamation-triangle-fill" />
    </svg>
    <div><h4><%=message%></h4></div>
  </div>
</div>
